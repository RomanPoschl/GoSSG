{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:css": "npx @tailwindcss/cli -i ./src/style.css -o ./src/tailwind.css",
    "watch:css": "npx @tailwindcss/cli -i ./frontend/src/style.css -o ./frontend/src/tailwind.css --watch --cwd ../",
    "dev": "vite",
    "dev2": "npm run watch:css && vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "vite": "^7.1.7"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.13",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/vite": "^4.1.13",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13"
  }
}
